{
  "name": "npmweb/redirector",
  "description": "Redirector",
  "repositories": [
    { "type": "vcs", "url": "git@bitbucket.org:npmweb/laravel-base" },
    { "type": "vcs", "url": "git@github.com:npmweb/php-env-loader" },
    { "type": "vcs", "url": "git@github.com:npmweb/php-helpers" }
  ],
  "require": {
    "laravel/framework": "4.2.*",
    "npmweb/laravel-base": "dev-master@dev",
    "npmweb/php-env-loader": "dev-master@dev",
    "npmweb/php-helpers": "dev-master@dev"
  },
  "autoload": {
    "classmap": [
      "app/database/migrations",
      "app/database/seeds"
    ],
    "psr-4": {
      "NpmWeb\\": "app/src/NpmWeb"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "php artisan optimize"
    ],
    "post-update-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "post-create-project-cmd": [
      "php artisan key:generate"
    ]
  },
  "config": {
    "preferred-install": "dist"
  },
  "minimum-stability": "dev"
}
