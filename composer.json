{
	"name": "npmweb/redirector",
	"description": "Redirector",
	"repositories": [
		{
			"type": "composer",
			"url": "http://dev.northpointministries.net/composer"
		},
		{
			"type": "vcs",
			"url": "https://github.com/npmweb/laravel-auth"
		}
	],
	"require": {
		"laravel/framework": "4.1.*",
		"laravelbook/ardent": "2.4.*"
	},
	"autoload": {
		"classmap": [
			"app/commands",
			"app/database/migrations",
			"app/database/seeds",
			"app/tests/TestCase.php"
		],
		"psr-4": {
			"NpmWeb\\": "app/src/NpmWeb"
		}
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "dev"
}
